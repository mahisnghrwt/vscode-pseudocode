{
    "name": "vscode-vc",
    "displayName": "vscode-vc",
    "description": "Syntax highlighting for vc",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "vc",
                "aliases": [
                    "vc"
                ],
                "extensions": [
                    ".vc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vc",
                "scopeName": "source.vc",
                "path": "./syntaxes/vc.tmLanguage.json"
            }
        ]
    }
}
